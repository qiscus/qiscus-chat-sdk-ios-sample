target 'Example' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Example
  pod 'QiscusUI', :path => '../QiscusUI/'
#  pod 'QiscusUI', '~> 0.2.1'

  # 3rd party
  pod 'SDWebImage', '~> 4.4.2'
